$system
molecule qchem_fchk
basis_file ../tests/qchem/EE_cart.fchk
$end

$perturb_cap
method EOM
package qchem
qchem_fchk ../tests/qchem/EE_cart.fchk
qchem_output ../tests/qchem/EE_cart.out
nstates 3
CAP_TYPE box
CAP_X 2.76
CAP_Y 2.76
CAP_Z 4.88
$end

$job
title Q-Chem EOMEE with cartesian GTOs specified via fchk
jobtype perturb_cap
$end

