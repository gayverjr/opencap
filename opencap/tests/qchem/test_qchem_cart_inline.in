$geometry
N        0.0     0.0     1.039
N        0.0     0.0     -1.039
X       0.0     0.0     0.0
$end

$system
molecule inline
basis_file ../tests/data/test_basis.bas
bohr_coordinates true
cart_bf  dfg
$end

$perturb_cap
method EOM
package qchem
qchem_fchk ../tests/qchem/EE_cart.fchk
qchem_output ../tests/qchem/EE_cart.out
nstates 3
CAP_TYPE box
CAP_X 2.76
CAP_Y 2.76
CAP_Z 4.88
Radial_precision  14
Angular_points    110
$end

$job
title Q-Chem EOMEE with cartesian GTOs specified inline
jobtype perturb_cap
$end

